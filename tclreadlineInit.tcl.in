package provide tclreadline @TCLREADLINE_VERSION@

namespace eval tclreadline:: {

    proc Init {} {
        catch {load @TERMCAP_LOAD_PATH@}
        catch {load @READLINE_LOAD_PATH@}
        if {[catch {load @TCLREADLINE_LIB_FILE@} msg]} {
            puts stderr $msg
            exit 2
        }
    }

}
